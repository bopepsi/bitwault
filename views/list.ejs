          <h2>Nothing here...Maybe <a href="/new" data-linkto="new">create</a> some?</h2>
        <% } else { %>
          <% for(const item of webURL_arr) { %> 
            <%- include('./partials/_webinfo_displayOnly_card', {webURL_item: item}) %> 
          <% } %> 
        <% } %> 
      </ul>
      </section>
    </main>
    <%- include('./partials/_footer') %> 
  </body>
</html>
